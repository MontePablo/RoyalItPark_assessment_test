            android:id="@+id/backround"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scaleX="100" />
        <Constraint
            android:id="@+id/backround"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scaleX=".3" />
        <Constraint
            android:id="@+id/backround"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
