            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scaleX="0" />
        <Constraint
            android:id="@+id/backround"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scaleX="10" />
        <Constraint
            android:id="@+id/backround"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scaleX="1" />
        <Constraint
            android:id="@+id/backround"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scaleX="0" />
        <Constraint
            android:id="@+id/backround"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scaleX="10" />
        <Constraint
            android:id="@+id/backround"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scaleY="10" />
