            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scaleY=".1" />
        <Constraint
            android:id="@+id/backround"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scaleX=".1" />
        <Constraint
            android:id="@+id/backround"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scaleX=".2" />
        <Constraint
            android:id="@+id/backround"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scaleX=".3" />
        <Constraint
            android:id="@+id/backround"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
